# Example Render environment variables (replace placeholders before adding to Render)
SECRET_KEY=replace_with_a_secure_random_value
JWT_SECRET_KEY=replace_with_a_secure_random_value
SQLALCHEMY_DATABASE_URI=postgresql://<DB_USER>:<DB_PASSWORD>@<DB_HOST>:<DB_PORT>/<DB_NAME>
DATABASE_URL=postgresql://<DB_USER>:<DB_PASSWORD>@<DB_HOST>:<DB_PORT>/<DB_NAME>
FLASK_ENV=production
FLASK_DEBUG=False
PYTHONUNBUFFERED=1
WORKERS=2

